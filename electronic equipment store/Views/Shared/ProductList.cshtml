@model List<electronic_equipment_store.Models.Product>

@foreach (var product in Model)
{
    <div>
        <h3>@product.Name</h3>
        <p>@product.Description</p>
        <p>Цена: @product.Price</p>
        <img src="@product.ImageUrl" alt="@product.Name" style="max-width: 150px; max-height: 150px;" />
    </div>
}
